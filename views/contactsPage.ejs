<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Your Contacts</title>
  </head>
  <body>
    <h1>All Contacts:</h1>
    <ul>
      <% contacts.forEach(function(contact) { %>
      <h1><%= contact.firstName %></h1>
      <% }) %>
    </ul>
  </body>
</html>
